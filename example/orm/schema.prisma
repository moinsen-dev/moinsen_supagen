model CmeEdges {
  color String
  createdAt Datetime
  description String
  direction String @default(autoincrement())
  id String @default(autoincrement()) @id
  name String @default(autoincrement())
  thickness Int
  updatedAt Datetime
}

model CmeIdeas {
  createdAt Datetime
  description String
  id String @default(autoincrement()) @id
  title String @default(autoincrement())
}

model CmeItems {
  attachments String[]
  category String
  classification String
  createdAt Datetime
  data String
  description String
  endDate Datetime
  id String @default(autoincrement()) @id
  idLocation Int
  idUser String @default(autoincrement())
  notes String[]
  origin String
  priority String
  startDate Datetime
  tags String[]
