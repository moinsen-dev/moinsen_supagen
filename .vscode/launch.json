  "version": "0.2.0",
  "configurations": [
    {
      "name": "supagen-db",
      "request": "launch",
      "type": "dart",
      "args": ["db", "-o example/orm"],
      "program": "bin/moinsen_supagen.dart"
    },
    {
      "name": "supagen-dart-convert",
      "request": "launch",
      "type": "dart",
      "args": [
        "convert",
        "-g dart",
        "-o example/lib/entities",
        "-i example/json/table-infos.json"
      ],
      "program": "bin/moinsen_supagen.dart"
    },
    {
      "name": "supagen-prisma-convert",
      "request": "launch",
      "type": "dart",
      "args": [
        "convert",
        "-g prisma",
        "-o example/orm",
        "-i example/json/table-infos.json"
      ],
      "program": "bin/moinsen_supagen.dart"
    },
    {
      "name": "supagen-erd-convert",
      "request": "launch",
      "type": "dart",
      "args": [
        "convert",
        "-g erd",
        "-o example/orm",
        "-i example/json/table-infos.json"
      ],
      "program": "bin/moinsen_supagen.dart"
    }
  ]
}
